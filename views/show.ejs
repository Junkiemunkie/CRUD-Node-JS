<html>
     <head>
        <title></title>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
     </head>
     <body>
        <div class="container">
            <table class="table table-bordered table-border text-center shadow">
                <tr>
                    <th>user id</th>
                    <th>user name</th>
                    <th>user email</th>
                    <th>user password</th>
                    <th>user delete</th>
                    <th>user update</th>
                </tr>

                <% users.forEach(function(user){ %>
                <tr>
                    <td><%= user.user_id%></td>
                    <td><%= user.user_name%></td>
                    <td><%= user.user_email%></td>
                    <td><%= user.user_password%></td>
                    
                    <td><a href="/delete/<%= user.user_id%>" class="btn btn-danger btn-sm">deleted</a></td>
                    <td><a href="/update/<%= user.user_id%>" class="btn btn-success btn-sm">Update</a></td>
                </tr>
                <% }) %>
            </table>
        </div>

     </body>
</html>